<marlin>
    <constants>
    </constants>

    <execute>
        <processor name="MyStatusMonitor" />
    </execute>

    <global>
        <parameter name="LCIOInputFiles">
            filename.slcio
        </parameter>
        <parameter name="MaxRecordNumber" value="0"/>
